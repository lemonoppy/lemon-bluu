{
  "name": "kaiju-keeper",
  "version": "1.1.0",
  "private": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start:built": "node -r tsconfig-paths/register build/index.js",
    "start:dev": "NODE_ENV=development & nodemon -r tsconfig-paths/register --files src/index.ts | pino-pretty -i pid,hostname - t 'yyyy-mm-dd HH:MM:ss'",
    "start:prod": "ts-node -r tsconfig-paths/register --files src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "import-seasons": "node scripts/import-seasons.js",
    "patch-package": "patch-package"
  },
  "dependencies": {
    "@lemon-bluu/discord": "*",
    "@discordjs/rest": "^2.2.0",
    "@google-cloud/local-auth": "3.0.1",
    "@google/genai": "^1.43.0",
    "@keyv/sqlite": "^4.0.5",
    "axios": "^1.12.0",
    "cheerio": "^1.0.0",
    "cron": "^4.4.0",
    "csv-parse": "^6.1.0",
    "discord.js": "^14.19.3",
    "dotenv": "^17.3.1",
    "fuzzysort": "^3.0.2",
    "google-spreadsheet": "^5.2.0",
    "googleapis": "171",
    "jsdom": "^28.1.0",
    "keyv": "^5.6.0",
    "keyv-file": "^5.3.3",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "neverthrow": "^8.2.0",
    "pg": "^8.16.0",
    "puppeteer": "^24.15.0",
    "tsc-alias": "^1.8.11"
  },
  "devDependencies": {
    "@lemon-bluu/eslint-config": "*",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^25.3.2",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.0",
    "nodemon": "^3.0.2",
    "patch-package": "^8.0.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "postinstall-postinstall": "^2.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.2"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ignore": [
      "build/*",
      ".vscode/*"
    ],
    "delay": "2500"
  }
}
